    "import pygame\n",
    "try:\n",
    "    frame += 1\n",
    "except NameError as e:\n",
    "    frame = 0\n",
    "normal_font = pygame.font.Font(None, 50)\n",
    "text_surf = normal_font.render(str(frame), False, \"White\")\n",
    "text_rect = text_surf.get_rect(center = (400, 300))\n",
    "screen.fill(\"Black\")\n",
    "screen.blit(text_surf, text_rect)"
   ]
